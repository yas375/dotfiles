# cd
alias ..='cd ..'

# ls
alias less='less -R'

alias pwgen='pwgen -1cnsB'
alias cdtemp='cd `mktemp -d`'

# rails
alias trt='touch tmp/restart.txt'

# CocoaPods
alias pod-dev='COCOA_PODS_ENV=development $HOME/code/Rainforest/CocoaPods/bin/pod'

# Having one more dropbox account
alias dropbox-work='HOME=$HOME/Carezone /Applications/Dropbox.app/Contents/MacOS/Dropbox &'

alias subl="/Applications/Sublime\ Text.app/Contents/SharedSupport/bin/subl"

# More "l"s => more detail
alias gl='git log --oneline --decorate'
alias gll='git log --oneline --decorate --graph'
alias glll='git log --decorate --graph'

